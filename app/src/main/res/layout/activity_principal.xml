<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.smartpark.smartpark.PrincipalActivity">

    <ImageButton
        android:id="@+id/btnMeusVeiculos"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="27dp"
        android:layout_marginStart="16dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/btnNovoVeiculo"
        app:layout_constraintEnd_toStartOf="@+id/btnNovoVeiculo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnNovoVeiculo"
        app:srcCompat="@drawable/ic_carro" />

    <ImageButton
        android:id="@+id/btnNovoAgendamento"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="26dp"
        android:layout_marginEnd="161dp"
        android:layout_marginTop="21dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/textView23"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnAlterarCadastro"
        app:layout_constraintTop_toBottomOf="@+id/textView21"
        app:srcCompat="@drawable/ic_adicionar_circulo" />

    <ImageButton
        android:id="@+id/btnConsultarAgendamento"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="36dp"
        android:layout_marginTop="16dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/textView21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnNovoVeiculo"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_consultar" />

    <ImageButton
        android:id="@+id/btnAlterarCadastro"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="27dp"
        android:layout_marginStart="16dp"
        android:adjustViewBounds="false"
        android:background="@android:color/transparent"
        android:padding="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnNovoAgendamento"
        app:layout_constraintEnd_toStartOf="@+id/btnNovoAgendamento"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnNovoAgendamento"
        app:srcCompat="@drawable/ic_pessoa" />

    <ImageButton
        android:id="@+id/btnNovoVeiculo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="35dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/btnConsultarAgendamento"
        app:layout_constraintEnd_toStartOf="@+id/btnConsultarAgendamento"
        app:layout_constraintStart_toEndOf="@+id/btnMeusVeiculos"
        app:layout_constraintTop_toTopOf="@+id/btnConsultarAgendamento"
        app:srcCompat="@drawable/ic_carro" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="19dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="101dp"
        android:text="Meus Veículos"
        app:layout_constraintEnd_toStartOf="@+id/textView20"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Novo Veículo"
        app:layout_constraintEnd_toStartOf="@+id/textView21"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/textView19" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="3dp"
        android:text="Consultar Agendamentos"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView20" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="225dp"
        android:text="Alterar Cadastro"
        app:layout_constraintStart_toStartOf="@+id/btnAlterarCadastro"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="Novo Agendamento"
        app:layout_constraintStart_toEndOf="@+id/textView22"
        app:layout_constraintTop_toTopOf="@+id/textView22" />
</android.support.constraint.ConstraintLayout>
